<% include header %>
<div class="container">
  <div class="d-flex justify-content-center">
    <div class="form-tt">
      <h2>Thay đổi mật khẩu của bạn</h2>
      <form action="/user/changepassword" method="post" name="dang-ky">
        <input
          type="text"
          name="newPassword"
          id="pwd"
          placeholder="Nhập mật khẩu mới"
        />

        <input type="submit" name="submit" value="Xác nhận" />

        <a class="btn btn-success mt-4" href="/"
          >Quay lại trang chủ <i class=""></i
        ></a>
      </form>
    </div>
  </div>
</div>
